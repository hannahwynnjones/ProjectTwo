<h1>Rate My Bake Users</h1>

<% if(locals.isAuthenticated) { %>

<% users.forEach((user) => { %>
  <div class="index">

    <a href="/users/<%= user.id %>">
    <img id="profilePic" src="<%= user.ppImage %>" alt="<%= user.username %>">
    <img id="profilePic" src="<%= user.image %>">


      <h2 class="index"><%= user.username %></h2>
    </a>

  </div>
<% }) %>
<% } %>
